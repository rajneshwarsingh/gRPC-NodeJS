{
  "name": "gRPC-NodeJS",
  "version": "1.0.0",
  "description": "The project represents a remote procedure call with the help of gRPC module in typescript.",
  "main": "index.js",
  "scripts": {
    "start:dev": "npm run build && node dist/index.js",
    "build": "tsc --project .",
    "compile": "pbjs ./src/proto/*.proto --es5 ./dist/proto.js",
    "copy": "cp -R ./src/proto ./dist/src/",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajneshwarsingh/gRPC-NodeJS.git"
  },
  "author": "Rajneshwar Singh",
  "license": "MIT",
  "keywords": [
    "Node.js",
    "Typescript",
    "gRPC"
  ],
  "bugs": {
    "url": "https://github.com/rajneshwarsingh/gRPC-NodeJS/issues"
  },
  "homepage": "https://github.com/rajneshwarsingh/gRPC-NodeJS#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.4",
    "@grpc/proto-loader": "^0.7.12",
    "pbjs": "0.0.14",
    "prettier": "^3.2.5",
    "protobufjs": "^7.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0"
  }
}